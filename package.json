{
	"name": "SalmonStatHQ",
	"version": "0.1.1-beta.10",
	"description": "Splatoon2のSalmonRun記録用アプリ",
	"private": true,
	"dependencies": {
		"electron-is-dev": "^0.3.0"
	},
	"build": {
		"appId": "com.kent-and-co.salmonstathq",
		"mac": {
			"target": "zip",
			"icon": "./icondata/salmonhq.icns"
		},
		"win": {
			"target": {
				"target": "nsis",
				"arch": [
					"x64",
					"ia32"
				]
			},
			"icon": "./icondata/salmonhq.ico"
		},
		"publish": [
			{
				"provider": "github",
				"releaseType": "release",
				"owner": "sasagar",
				"publishAutoUpdate": "true"
			}
		],
		"electronVersion": "2.0.2",
		"files": [
			"!release",
			"!original",
			"!dist"
		],
		"directories": {
			"buildResources": "icondata"
		}
	},
	"scripts": {
		"start": "nf start -p 3000",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"electron": "electron .",
		"electron-start": "node src/electron-wait-react",
		"react-start": "react-scripts start",
		"pack": "build --dir",
		"dist": "npm run build && build",
		"postinstall": "install-app-deps",
		"dev": "nf start -p 3000",
		"release": "build -mw"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"electron": "^2.0.2",
		"electron-builder": "^20.15.1",
		"electron-packager": "^12.1.0",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-eslint": "^4.0.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.9.1",
		"foreman": "^3.0.0",
		"prop-types": "^15.6.1",
		"react": "^16.4.0",
		"react-dom": "^16.4.0",
		"react-scripts": "1.1.4",
		"webpack": "^3.11.0",
		"webpack-cli": "^2.1.5"
	},
	"main": "build/electron.js",
	"homepage": "./",
	"repository": "github:sasagar/salmonstatHQ",
	"author": "SASAGAWA, Kiyoshi",
	"license": "GPL-3.0"
}
